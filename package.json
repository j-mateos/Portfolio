{
    "name": "portfolio",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "bun vite .",
        "format": "bun prettier . --write --log-level warn",
        "lint": "bun eslint . --ext .js,.jsx --fix",
        "fix": "bun run format && bun run lint",
        "test": "bun vitest .",
        "build": "bun vite build .",
        "preview": "bun run build && bun vite preview .",
        "deps-check": "bunx knip",
        "deps-audit": "bun audit",
        "clean-build": "rm -rf dist",
        "clean-deps": "rm -rf node_modules bun.lock"
    },
    "dependencies": {
        "@vercel/speed-insights": "1.3.1",
        "i18next": "25.7.2",
        "i18next-http-backend": "3.0.2",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "react-i18next": "16.4.0"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "6.9.1",
        "@testing-library/react": "16.3.0",
        "@types/react": "19.1.8",
        "@types/react-dom": "19.1.6",
        "@vitejs/plugin-react-swc": "3.10.2",
        "@vitest/ui": "4.0.8",
        "eslint": "8.57.1",
        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-prettier": "5.5.5",
        "eslint-plugin-react": "7.37.5",
        "eslint-plugin-react-hooks": "7.0.1",
        "eslint-plugin-vitest": "0.5.4",
        "jsdom": "26.0.0",
        "vite": "6.4.1",
        "vitest": "4.0.8"
    }
}
